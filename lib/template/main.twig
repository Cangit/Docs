    <style>
    body {
  font-size: 14px;
  margin: 0; }

header .logoContainer {
  margin: 0 auto;
  padding-left: 100px;
  width: 100px; }
  header .logoContainer .logo {
    background: url("/resources/_docs/img/royal-logo.png");
    background-size: 100px 180px;
    width: 100px;
    height: 180px;
    margin-top: 20px; }
header h2 {
  font-family: "Gabriela", serif;
  font-size: 20px;
  margin: 0 auto;
  text-align: center;
  color: #d45500;
  padding: 5px 0;
  padding-left: 100px; }
header nav {
  position: absolute;
  top: 0;
  left: 0;
  width: 100px;
  font-family: Arial; }
  header nav ul {
    margin: 15px 5px;
    padding: 0; }
    header nav ul li {
      display: block; }
      header nav ul li a {
        display: block;
        padding: 4px 7px;
        color: #000;
        text-decoration: none; }
        header nav ul li a:hover {
          background: #d45500;
          color: #fff; }
    header nav ul .selected {
      background: #d45500; }
      header nav ul .selected a {
        color: #fff; }
  header nav h2 {
    font-family: Arial;
    text-align: left;
    padding-left: 5px;
    color: #000; }
  header nav h3 {
    padding-left: 5px; }

#content h1 {
  font-family: arial;
  padding-left: 10px; }
  #content h1 .small {
    color: #ccc;
    font-size: 0.5em; }
#content .noRoutes {
  text-align: center;
  font-family: Arial;
  margin-top: 30px;
  font-size: 2em;
  color: #29404e; }
#content .openFile {
  display: block;
  padding: 10px;
  border-bottom: 1px dotted #cccccc;
  background: #eee;
  margin-top: 0px;
  color: #999988;
  font-family: monospace;
  text-decoration: none; }
  #content .openFile:hover .fileLocationPath {
    color: #e33d44; }
  #content .openFile .fileLocationPath {
    color: #445588; }
#content .rawCode {
  padding: 0 10px; }
#content .controllerEntry {
  display: none;
  padding: 10px 20px;
  background: #bec3c7; }
  #content .controllerEntry input {
    margin: 10px;
    padding: 5px 20px;
    font-size: 1.5em;
    border: 0;
    width: 300px;
    outline: none; }
#content .analyzerMenu {
  width: 100%;
  height: 35px;
  overflow: hidden;
  background: #34495e;
  font-family: Arial;
  font-size: 1.2em;
  text-transform: uppercase; }
  #content .analyzerMenu .wrapper {
    padding: 0 10px; }
    #content .analyzerMenu .wrapper a {
      display: inline-block;
      text-decoration: none;
      color: #fff;
      padding: 10px 20px 20px 20px;
      border-left: 1px solid #2d3e50;
      border-right: 1px solid #2d3e50; }
      #content .analyzerMenu .wrapper a:hover {
        background: #2d3e50; }
    #content .analyzerMenu .wrapper a.left {
      float: left; }
    #content .analyzerMenu .wrapper a.right {
      float: right; }
#content .routes {
  margin: 0 10px; }
  #content .routes .route {
    font-family: monospace;
    font-size: 1.2em;
    margin: 20px 0;
    background: #ecf0f1;
    padding-bottom: 5px; }
    #content .routes .route .path {
      font-size: 1.3em;
      padding: 5px 20px; }
      #content .routes .route .path .createController, #content .routes .route .path .viewController {
        float: right;
        font-size: 0.6em;
        padding: 2px 10px; }
      #content .routes .route .path .createController {
        background: no-repeat url(/resources/_docs/img/add.png);
        background-size: 32px 22px;
        width: 32px;
        height: 22px;
        padding: 0;
        margin: 2px 0px;
        cursor: pointer; }
      #content .routes .route .path .viewController {
        background: no-repeat url(/resources/_docs/img/view.png);
        background-size: 32px 22px;
        width: 32px;
        height: 22px;
        padding: 0;
        margin: 2px 0px;
        cursor: pointer; }
    #content .routes .route .exists.path {
      background: #27ae61;
      color: #fff; }
    #content .routes .route .missing.path {
      background: #bec3c7;
      color: #2c3e4e; }
    #content .routes .route .error.path {
      background: #c1392a;
      color: #fff; }
    #content .routes .route .controller {
      background: no-repeat url(/resources/_docs/img/cog.png);
      background-size: 16px;
      margin-left: 20px;
      padding-left: 25px;
      margin-top: 10px; }
    #content .routes .route .phpFile {
      background: no-repeat url(/resources/_docs/img/page_white_php.png);
      background-size: 16px;
      margin-left: 20px;
      padding-left: 25px;
      color: #34495e;
      text-decoration: none; }
      #content .routes .route .phpFile:hover {
        text-decoration: underline; }
    #content .routes .route .missing.phpFile {
      color: #95a5a5; }
    #content .routes .route .methods {
      background: no-repeat url(/resources/_docs/img/bricks.png);
      background-size: 16px;
      margin-left: 20px;
      padding-left: 25px; }
      #content .routes .route .methods .unimportant {
        color: #95a5a5;
        font-size: 0.9em; }
    #content .routes .route .information {
      background: #bec3c7;
      padding: 10px 5px 10px 20px; }
    #content .routes .route .alert {
      color: #fff;
      background: #e84c3d; }
#content article {
  font-family: Arial;
  width: 700px;
  margin: 50px auto;
  padding-left: 100px; }
  #content article h1 {
    text-align: center;
    font-size: 3em;
    color: #333;
    border-bottom: 3px solid #d45500;
    font-family: "Gabriela", serif; }
  #content article h2 {
    font-size: 2em;
    margin: 30px 0 10px 0;
    color: #333; }
  #content article p {
    font-size: 1.1em;
    line-height: 1.4em; }
  #content article p.tip {
    background: #fdf1da;
    border: 1px solid #f5e4c9;
    padding: 10px; }
  #content article p.parameters {
    background: #9b58b5;
    color: #fff;
    border: 1px solid #9144ae;
    padding: 10px; }
  #content article p.response {
    background: #3997db;
    color: #fff;
    border: 1px solid #2f80b9;
    padding: 10px; }
    #content article p.response a {
      color: #fff; }
  #content article a {
    color: #d45500; }
  #content article ul li {
    padding: 5px 0;
    font-size: 1.1em;
    line-height: 1.4em; }
  #content article pre {
    border-top: 1px dashed black;
    border-bottom: 1px dashed black;
    padding: 5px 10px;
    background-color: #f8f8f8;
    overflow-x: auto; }
  #content article b {
    font-weight: bold; }
  #content article em {
    font-style: italic; }
    

pre code {
  display: block; padding: 0.5em;
  color: #333;
  background: #f8f8ff
}

pre .comment,
pre .template_comment,
pre .diff .header,
pre .javadoc {
  color: #998;
  font-style: italic
}

pre .keyword,
pre .css .rule .keyword,
pre .winutils,
pre .javascript .title,
pre .nginx .title,
pre .subst,
pre .request,
pre .status {
  color: #333;
  font-weight: bold
}

pre .number,
pre .hexcolor,
pre .ruby .constant {
  color: #099;
}

pre .string,
pre .tag .value,
pre .phpdoc,
pre .tex .formula {
  color: #d14
}

pre .title,
pre .id {
  color: #900;
  font-weight: bold
}

pre .javascript .title,
pre .lisp .title,
pre .clojure .title,
pre .subst {
  font-weight: normal
}

pre .class .title,
pre .haskell .type,
pre .vhdl .literal,
pre .tex .command {
  color: #458;
  font-weight: bold
}

pre .tag,
pre .tag .title,
pre .rules .property,
pre .django .tag .keyword {
  color: #000080;
  font-weight: normal
}

pre .attribute,
pre .variable,
pre .lisp .body {
  color: #008080
}

pre .regexp {
  color: #009926
}

pre .class {
  color: #458;
  font-weight: bold
}

pre .symbol,
pre .ruby .symbol .string,
pre .lisp .keyword,
pre .tex .special,
pre .prompt {
  color: #990073
}

pre .built_in,
pre .lisp .title,
pre .clojure .built_in {
  color: #0086b3
}

pre .preprocessor,
pre .pi,
pre .doctype,
pre .shebang,
pre .cdata {
  color: #999;
  font-weight: bold
}

pre .deletion {
  background: #fdd
}

pre .addition {
  background: #dfd
}

pre .diff .change {
  background: #0086b3
}

pre .chunk {
  color: #aaa
}
